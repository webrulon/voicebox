{
  "$schema": "https://schema.tauri.app/config/2",
  "identifier": "default",
  "description": "Default permissions for voicebox",
  "platforms": ["linux", "macOS", "windows"],
  "windows": ["main"],
  "remote": {
    "urls": ["http://localhost:*"]
  },
  "permissions": [
    "core:default",
    "core:window:default",
    "core:window:allow-start-dragging",
    "core:webview:default",
    "core:webview:allow-internal-toggle-devtools",
    "shell:allow-open",
    "shell:allow-execute",
    "shell:allow-spawn",
    "updater:default",
    "process:default",
    "dialog:default",
    "dialog:allow-save",
    "dialog:allow-open",
    "fs:default",
    "fs:read-all",
    "fs:write-all"
  ]
}
